{
    "name": "netofingsweb",
    "version": "1.0.0",
    "description": "A simple IoT platform",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=production node server.js",
        "prestart": "npm run build",
        "start-dev": "DEBUG=netofings:* nodemon server.js",
        "prestart-dev": "npm run build",
        "build": "browserify -t babelify -t vueify client/app.js -o public/bundle.js",
        "lint": "standard"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FranciscoGustavo/netofings.git"
    },
    "author": "FrcGustavo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FranciscoGustavo/netofings/issues"
    },
    "homepage": "https://github.com/FranciscoGustavo/netofings#readme",
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015"
                    ]
                }
            ]
        ]
    },
    "dependencies": {
        "chalk": "^3.0.0",
        "chart.js": "^2.9.4",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "express-asyncify": "^1.0.0",
        "netofingsagent": "file:../netofingsagent",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.8",
        "socket.io": "^2.3.0",
        "vue": "^2.6.11",
        "vue-chartjs": "^3.5.0"
    },
    "devDependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^10.0.0",
        "browserify": "^16.5.0",
        "nodemon": "^2.0.2",
        "standard": "^14.3.1",
        "vueify": "^9.4.1"
    }
}
